<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

	<!--Set this to true if you want to generate all necessary/dependent DLLs in order to use them in Unity Project for example-->
	<PropertyGroup>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
	</PropertyGroup>

	
	<ItemGroup>
	  <ProjectReference Include="..\Substrate.Bajun.NET.NetApiExt\Substrate.Bajun.NET.NetApiExt.csproj" />
	</ItemGroup>

</Project>
